{
  "name": "xlombard",
  "version": "0.0.2",
  "private": true,
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "dev": "vite",
    "build-check": "run-p type-check build-only",
    "build": "vite build && cp dist/index.html dist/200.html && cp config/netlify.toml dist/netlify.toml",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint:js": "eslint . -c .eslintrc.cjs --ext \".vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts\" --ignore-path .gitignore",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js --fix && npm run lint:style --fix"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^12.0.0",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "floating-vue": "^5.2.2",
    "grid-layout-plus": "^1.0.6",
    "lodash": "^4.17.21",
    "maska": "^3.1.0",
    "moment": "^2.30.1",
    "nanoid": "^5.0.9",
    "ofetch": "^1.4.1",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.1.3",
    "tiny-emitter": "^2.1.0",
    "v-calendar": "^3.1.2",
    "vee-validate": "^4.14.7",
    "vue": "^3.5.13",
    "vue-file-agent": "^1.7.3",
    "vue-router": "^4.5.0",
    "vue-select": "^4.0.0-beta.6",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/lodash": "^4.17.13",
    "@types/node": "22.10.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^9.1.0",
    "eslint": "8.57.1",
    "eslint-plugin-vue": "^9.32.0",
    "estree-walker-ts": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.7.0",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "stylelint": "^16.11.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^6.0.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.1.10"
  }
}
